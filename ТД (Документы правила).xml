		КонецЕсли;
		
		Если Окр(Строка.Количество, 2) = 0.00 Тогда
			Продолжить;
		КонецЕсли;
				
		СтрокаСообщения = "";
		Если Окр(Строка.Количество, 2) &lt; 0.00 Тогда
			СтрокаСообщения = НСтр("ru='Обнаружен отрицательный остаток на складе ""%1"" по товару ""%2"" при выгрузке по правилу: ""%3"" %6'");
		ИначеЕсли Не ЗначениеЗаполнено(Строка.Сумма) Или Окр(Строка.Сумма, 2) &lt;= 0.00 Или Окр(Строка.Сумма / Строка.Количество, 2) &lt;= 0.00 Тогда
			СтрокаСообщения = НСтр("ru='Обнаружен некорректный суммовой остаток на складе ""%1"" по товару ""%2"" при выгрузке по правилу: ""%3"" %6'");
		КонецЕсли;
		
		Если ЗначениеЗаполнено(СтрокаСообщения) Тогда
			СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%1", Строка.Склад);
			СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%2", Строка.Номенклатура);
			СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%3", НаименованиеТекущегоПВД);
			СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%4", Строка.Организация);
			СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%5", Строка.Количество);
			Если ЗначениеЗаполнено(Строка.Организация) Тогда
				СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%6", НСтр("ru='по организации '") + Строка.Организация);
			Иначе
				СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%6", НСтр("ru='по управленческим данным.'"));
			КонецЕсли;
			
			ЗаписатьВПротоколВыполнения(СтрокаСообщения,,Ложь);
			Сообщить("АЛГОРИТМ ДобавитьСтрокиВыборкиДанныхПереданныеВПереработку условие1: " + СтрокаСообщения);
			Продолжить;
		КонецЕсли;

		НоваяСтрокаТовары = НоваяСтрока.Товары.Добавить();
		ЗаполнитьЗначенияСвойств(НоваяСтрокаТовары, Строка);
		НоваяСтрокаТовары.Цена = Строка.Сумма / Строка.Количество;
		НоваяСтрокаТовары.КоличествоУпаковок = Строка.Количество;
		
		Если ТаблицаОстатков.Колонки.Найти("НомерГТД") &lt;&gt; Неопределено И ЗначениеЗаполнено(Строка.НомерГТД)
			//++КАКЗ
			И НЕ Параметры.какз_ВыгружатьДанныеУчетаИсточниковГТД=Истина
			//--КАКЗ
			
			Тогда
			НоваяСтрокаТовары.НомерГТД = Новый Структура;
			НоваяСтрокаТовары.НомерГТД.Вставить("Владелец",            Строка.Номенклатура);
			НоваяСтрокаТовары.НомерГТД.Вставить("СтранаПроисхождения", Строка.СтранаПроисхождения);
			НоваяСтрокаТовары.НомерГТД.Вставить("ПометкаУдаления",     Строка.ПометкаУдаления);
			НоваяСтрокаТовары.НомерГТД.Вставить("Код",                 Строка.НомерГТД);
		КонецЕсли;

		// Запоминаем серии номенклатуры, если для нее ведется учет серий
		Если ТаблицаОстатков.Колонки.Найти("ВестиУчетПоСериям") &lt;&gt; Неопределено И Строка.ВестиУчетПоСериям Тогда
			НоваяСтрокаСерии = НоваяСтрока.Серии.Добавить();
			НоваяСтрокаСерии.КоличествоУпаковок = Строка.Количество;
			ЗаполнитьЗначенияСвойств(НоваяСтрокаСерии, Строка);
		КонецЕсли;		
		
	Исключение

		ТекстОшибки = ОписаниеОшибки();
		Если ТекстОшибки &lt;&gt; "" Тогда
			ЗаписатьВПротоколВыполнения(ТекстОшибки,,Ложь);
		КонецЕсли;	
		Сообщить("АЛГОРИТМ ДобавитьСтрокиВыборкиДанныхПереданныеВПереработку исключение1: " + ТекстОшибки);
	КонецПопытки;

КонецЦикла;

// Если в документе нет строк с товарами, то удаляем его из выборки
Если ВыборкаДанных.Количество() &gt; 0 И (ВыборкаДанных.Получить(ВыборкаДанных.Количество() - 1).Товары = Неопределено
	Или ВыборкаДанных.Получить(ВыборкаДанных.Количество() - 1).Товары.Количество() = 0) Тогда
	
	ВыборкаДанных.Удалить(ВыборкаДанных.Получить(ВыборкаДанных.Количество() - 1));
КонецЕсли;

Индекс = ВыборкаДанных.Количество()-1;
Пока Индекс &gt;= 0 Цикл
	Строка = ВыборкаДанных.Получить(Индекс);
	
	Если Параметры.Организации.Количество()&gt; 0 И Параметры.Организации.Найти(Строка.Организация) = Неопределено Тогда
		//++КАКЗ
		Если НЕ ЗначениеЗаполнено(Строка.Организация) Тогда
			Продолжить;
		КонецЕсли;
		//--КАКЗ

		ВыборкаДанных.Удалить(Строка);
	КонецЕсли;
	
	Индекс = Индекс-1;
КонецЦикла;</Текст>
				</Алгоритм>
				<Алгоритм Имя="ДобавитьТаблицыВВыборкуДанных" ИспользуетсяПриЗагрузке="false">
					<Текст>Если ТипОперации = "ОстаткиТоваровПолученныхНаКомиссию"
		//++КАКЗ
		Тогда
	//Или ТипОперации = "ОстаткиВозвратнойТарыПереданнойКлиентам" Тогда
	//--КАКЗ
	// Для этих типов операций регл = упр
	ТаблицаОстатков = ТаблицаУпр;
	ТаблицаУпр = Неопределено;
ИначеЕсли ТипОперации = "ОстаткиМатериаловПереданныхВПроизводство" И ТаблицаУпр = Неопределено Тогда
	// При партионном учете всегда используются данные регл.
	ТаблицаОстатков = ТаблицаРегл;
ИначеЕсли Параметры.ИспользоватьДанныеТолькоРеглУчета Тогда
	ТаблицаОстатков = ТаблицаРегл;
ИначеЕсли Параметры.ИспользоватьДанныеТолькоУпрУчета Тогда
	ТаблицаОстатков = ТаблицаУпр;
	ТаблицаУпр = Неопределено;
Иначе
	Если Параметры.УчетПартийТолькоУУ Тогда
		ТаблицаОстатков = ТаблицаУпр;
		ТаблицаУпр = Неопределено;
	ИначеЕсли Параметры.УчетПартийТолькоБУ Тогда
		ТаблицаОстатков = ТаблицаРегл;
		ТаблицаУпр = Неопределено;
	Иначе
		МассивТаблиц = Новый Массив;
		МассивТаблиц.Добавить(ТаблицаУпр);
		МассивТаблиц.Добавить(ТаблицаРегл); 
		Если ТипОперации &lt;&gt; "ОстаткиТоваровПереданныхНаКомиссию" Тогда
			ЗаполнятьТаблицуНесопоставленных = Ложь;
		Иначе
			ЗаполнятьТаблицуНесопоставленных = Истина;
		КонецЕсли;

		Выполнить(Алгоритмы.ПосчитатьРазницуДобавитьВоВременные);
		
		ТаблицаОстатков = Параметры.ПараметрыАлгоритмов.ТаблицаРеглВрем;
		ТаблицаУпр     = Параметры.ПараметрыАлгоритмов.ТаблицаУпрВрем;
		Если ТипОперации = "ОстаткиТоваровПереданныхНаКомиссию" Тогда
			// Тут пустые суммы. Заполнены только количества. В отдельный документ надо занести.
			ТаблицаНесопоставлено = Параметры.ПараметрыАлгоритмов.ТаблицаРеглНесопоставленоВрем;
		КонецЕсли;
	КонецЕсли;
КонецЕсли;   
ВыборкаДанных = Новый ТаблицаЗначений;
ВыборкаДанных.Колонки.Добавить("ТипОперации");
ВыборкаДанных.Колонки.Добавить("Подразделение");
ВыборкаДанных.Колонки.Добавить("Дата");
ВыборкаДанных.Колонки.Добавить("Организация");
ВыборкаДанных.Колонки.Добавить("Склад");
ВыборкаДанных.Колонки.Добавить("Партнер");
ВыборкаДанных.Колонки.Добавить("Контрагент");
ВыборкаДанных.Колонки.Добавить("Договор");
ВыборкаДанных.Колонки.Добавить("ПредъявленСчетФактура");
ВыборкаДанных.Колонки.Добавить("НомерСчетаФактуры");
ВыборкаДанных.Колонки.Добавить("ДатаСчетаФактуры");
ВыборкаДанных.Колонки.Добавить("Товары");
ВыборкаДанных.Колонки.Добавить("Серии");
ВыборкаДанных.Колонки.Добавить("Комментарий");
ВыборкаДанных.Колонки.Добавить("СоглашениеСКомиссионером");
ВыборкаДанных.Колонки.Добавить("СоглашениеСКомитентом");
ВыборкаДанных.Колонки.Добавить("Валюта");
//++КАКЗ
ВыборкаДанных.Колонки.Добавить("Партия");
//--КАКЗ

Если ТаблицаОстатков.Колонки.Найти("ДокументОприходования") = Неопределено Тогда
	ТаблицаОстатков.Колонки.Добавить("ДокументОприходования");
КонецЕсли;
Если ТаблицаОстатков.Колонки.Найти("Партнер") = Неопределено Тогда
	ТаблицаОстатков.Колонки.Добавить("Партнер");
КонецЕсли;
Если ТаблицаОстатков.Колонки.Найти("НомерГТД") = Неопределено Тогда
	ТаблицаОстатков.Колонки.Добавить("НомерГТД");
КонецЕсли;
Если ТаблицаОстатков.Колонки.Найти("Склад") = Неопределено Тогда
	ТаблицаОстатков.Колонки.Добавить("Склад");
КонецЕсли;
Если ТаблицаОстатков.Колонки.Найти("СложныйУчетНДС") = Неопределено Тогда
	ТаблицаОстатков.Колонки.Добавить("СложныйУчетНДС");
КонецЕсли;
Если ТаблицаОстатков.Колонки.Найти("Договор") = Неопределено Тогда
	ТаблицаОстатков.Колонки.Добавить("Договор");
КонецЕсли;

Если ТаблицаОстатков.Колонки.Найти("ВестиУчетПоСериям") = Неопределено Тогда
	ТаблицаОстатков.Колонки.Добавить("ВестиУчетПоСериям");
	Для Каждого Строка Из ТаблицаОстатков Цикл
		Строка.ВестиУчетПоСериям = Параметры.ВыгружатьСерии;
	КонецЦикла;
КонецЕсли;
//++КАКЗ
Если Параметры.какз_ВыгружатьДанныеУчетаИсточниковГТД=Истина Тогда
	Выполнить(Алгоритмы.КАКЗ_РаспределитьОстаткиПоИсточникамПроисхождения);
КонецЕсли;
//--КАКЗ
Если ТипОперации = "ОстаткиМатериаловПереданныхПереработчикам" Тогда
	Выполнить(Алгоритмы.ДобавитьСтрокиВыборкиДанныхПереданныеВПереработку);
Иначе
	Выполнить(Алгоритмы.ДобавитьСтрокиВыборкиДанных);
КонецЕсли;
Если ТаблицаУпр &lt;&gt; Неопределено Тогда
	ДобавитьСерии = Ложь;
	Если ТаблицаУпр.Колонки.Найти("ВестиУчетПоСериям") = Неопределено Тогда
		ТаблицаУпр.Колонки.Добавить("ВестиУчетПоСериям");
		ДобавитьСерии = Истина;
	КонецЕсли;
	Если ТаблицаУпр.Колонки.Найти("Организация") = Неопределено Тогда
		ТаблицаУпр.Колонки.Добавить("Организация");
	КонецЕсли;
	Если ТаблицаУпр.Колонки.Найти("НомерГТД") = Неопределено Тогда
		ТаблицаУпр.Колонки.Добавить("НомерГТД");
	КонецЕсли;
	Если ТаблицаУпр.Колонки.Найти("Партнер") = Неопределено Тогда
		ТаблицаУпр.Колонки.Добавить("Партнер");
	КонецЕсли;
	Если ТаблицаУпр.Колонки.Найти("Договор") = Неопределено Тогда
		ТаблицаУпр.Колонки.Добавить("Договор");
	КонецЕсли;
	Если ТаблицаУпр.Колонки.Найти("СложныйУчетНДС") = Неопределено Тогда
		ТаблицаУпр.Колонки.Добавить("СложныйУчетНДС");
	КонецЕсли;
	Если ТаблицаУпр.Колонки.Найти("ДокументОприходования") = Неопределено Тогда
		ТаблицаУпр.Колонки.Добавить("ДокументОприходования");
	КонецЕсли;
	Если ТаблицаУпр.Колонки.Найти("СтавкаНДС") = Неопределено Тогда
		ТаблицаУпр.Колонки.Добавить("СтавкаНДС");
	КонецЕсли;
	Если ТаблицаУпр.Колонки.Найти("НДСРегл") = Неопределено Тогда
		ТаблицаУпр.Колонки.Добавить("НДСРегл");
	КонецЕсли;
	Если ТаблицаУпр.Колонки.Найти("СуммаНДС") = Неопределено Тогда
		ТаблицаУпр.Колонки.Добавить("СуммаНДС");
	КонецЕсли;
	
	Если ТаблицаУпр.Колонки.Найти("СуммаБезНДС") = Неопределено Тогда
		ТаблицаУпр.Колонки.Добавить("СуммаБезНДС");
	КонецЕсли;
	
	Если ТаблицаУпр.Колонки.Найти("СуммаРегл") = Неопределено Тогда
		ТаблицаУпр.Колонки.Добавить("СуммаРегл");
	КонецЕсли;
	
	Если ТаблицаУпр.Колонки.Найти("Склад") = Неопределено Тогда
		ТаблицаУпр.Колонки.Добавить("Склад");
	КонецЕсли;

	Для Каждого Строка Из ТаблицаУпр Цикл
		Строка.Организация = Справочники.Организации.ПустаяСсылка();
		
		Если ДобавитьСерии Тогда
			Строка.ВестиУчетПоСериям = Параметры.ВыгружатьСерии;
		КонецЕсли;
		
		Если Не ЗначениеЗаполнено(Строка.СтавкаНДС) Тогда
			Строка.СтавкаНДС = Строка.Номенклатура.СтавкаНДС;
			Если Не ЗначениеЗаполнено(Строка.СтавкаНДС) Тогда
				Строка.СтавкаНДС = Справочники.СтавкиНДС.ПустаяСсылка();
			КонецЕсли;			
		КонецЕсли;
		
		Если Не ЗначениеЗаполнено(Строка.Сумма) Тогда
			Продолжить; // не загружаем с нулевой суммовой оценкой и не рассчитываем ее. Т.к. сопоставление таких с регл. данными на предыдущем этапе не делали.
			// Если рассчитывать, то получим задвоение данных при переносе.
		КонецЕсли;		
		
		Если Не ЗначениеЗаполнено(Строка.НДСРегл) ИЛИ Не ЗначениеЗаполнено(Строка.СуммаНДС) Тогда
			//++БК3
			СтавкаНДСЧислом = УчетНДСиАкцизаВызовСервераПовтИсп.ПолучитьСтавкуНДС(Строка.СтавкаНДС) / 100;
			//--БК3
			
			Если Не ЗначениеЗаполнено(Строка.НДСРегл) Тогда
				Строка.НДСРегл  = Строка.СуммаРегл * СтавкаНДСЧислом;
			КонецЕсли;
			
			Если Не ЗначениеЗаполнено(Строка.СуммаНДС) Тогда
				Строка.СуммаНДС = Строка.СуммаБезНДС * СтавкаНДСЧислом;
				Строка.Сумма    = Строка.СуммаНДС + Строка.СуммаБезНДС;
			КонецЕсли;
			
		КонецЕсли;		
		
	КонецЦикла;
	
	//++НЕ УПП?
	Если ТаблицаУпр.Колонки.Найти("ДоговорКонтрагента") = Неопределено И ТаблицаУпр.Колонки.Найти("СчетУчета") &lt;&gt; Неопределено Тогда
		ТаблицаУпр.Свернуть("ВестиУчетПоСериям, Договор, ДокументОприходования, Качество, Номенклатура, НомерГТД, Организация, Партнер,"
							+ " Серия, Склад, СложныйУчетНДС, СтавкаНДС,СтатусПартии,СчетУчета, Характеристика, Подразделение",
							"Количество, НДСРегл, Сумма, СуммаБезНДС, СуммаНДС, СуммаРегл");
	КонецЕсли;
	//++КАКЗ
	ТаблицаОстатков = ТаблицаУпр;
	Если Параметры.какз_ВыгружатьДанныеУчетаИсточниковГТД=Истина Тогда
		Выполнить(Алгоритмы.КАКЗ_РаспределитьОстаткиПоИсточникамПроисхождения);
	КонецЕсли;
	//--КАКЗ
	
	Если ТипОперации = "ОстаткиМатериаловПереданныхПереработчикам" Тогда
		Выполнить(Алгоритмы.ДобавитьСтрокиВыборкиДанныхПереданныеВПереработку);
	Иначе
		Выполнить(Алгоритмы.ДобавитьСтрокиВыборкиДанных);
	КонецЕсли;
	Если ТипОперации = "ОстаткиТоваровПереданныхНаКомиссию" Тогда		
		ТаблицаОстатков = ТаблицаНесопоставлено;
		Выполнить(Алгоритмы.ДобавитьСтрокиВыборкиДанных);
	КонецЕсли;
КонецЕсли;</Текст>
				</Алгоритм>
				<Алгоритм Имя="КАКЗ_РаспределитьОстаткиПоИсточникамПроисхождения" ИспользуетсяПриЗагрузке="false">
					<Текст>	
	Если НЕ (ТипОперации = "ОстаткиТоваровПолученныхНаКомиссию"
		ИЛИ ТипОперации= "ОстаткиТоваровПереданныхНаКомиссию"
		ИЛИ ТипОперации= "ОстаткиВозвратнойТарыПринятойОтПоставщиков"
		ИЛИ ТипОперации= "ОстаткиВозвратнойТарыПереданнойКлиентам")
		Тогда
		
	//++УТП
	ВедетсяУчетПоСкладам = ?(ПланыСчетов.Типовой.Товары.ВидыСубконто.Найти(ПланыВидовХарактеристик.ВидыСубконтоТиповые.Склады, "ВидСубконто") = Неопределено, Ложь, Истина);
		
	//УчетнаяПолитикаОтсуствует = Ложь;
	//СтруктураУП = ОбщегоНазначения.ПолучитьПараметрыУчетнойПолитики(Дата("20221231235959"), УчетнаяПолитикаОтсуствует, , "Упр"); 
	//Если УчетнаяПолитикаОтсуствует Тогда
	//	ВедетсяУчетПоСкладам = Ложь;
	//Иначе
	//	ВедетсяУчетПоСкладам = СтруктураУП.ВестиУчетПоСкладамВРазрезеОрганизаций;
	//КонецЕсли;
	//--УТП
	
	Если Параметры.ПараметрыАлгоритмов.ДанныеУчетаТоваровОрганизацийПоИсточникамПроисхождения=Неопределено Тогда
		
		Запрос = Новый Запрос;
		Запрос.Текст = 
		"ВЫБРАТЬ
		|	ТоварыОрганизацийОстатки.Организация КАК Организация,
		|	ТоварыОрганизацийОстатки.Товар КАК Номенклатура,
		|	&amp;ХарактеристикаНоменклатуры КАК ХарактеристикаНоменклатуры,
		|	&amp;СерияНоменклатуры КАК СерияНоменклатуры,
		|	ТоварыОрганизацийОстатки.НомерГТД КАК НомерГТД,
		|	ТоварыОрганизацийОстатки.Склад КАК Склад,
		|	ТоварыОрганизацийОстатки.КоличествоОстаток,
		|	ВЫБОР
		|		КОГДА ТоварыОрганизацийОстатки.НомерГТД = ЗНАЧЕНИЕ(Справочник.НомераГТД.ПустаяСсылка)
		|			ТОГДА 1
		|		ИНАЧЕ ВЫБОР
		|				КОГДА ТоварыОрганизацийОстатки.НомерГТД.ПризнакПроисхождения = """"
		|					ТОГДА 2
		|				ИНАЧЕ 3
		|			КОНЕЦ
		|	КОНЕЦ КАК Приоритет
		|ИЗ
		|	РегистрНакопления.ТоварыОрганизацийБУ.Остатки(
		|			&amp;Период,
		|			Организация В (&amp;Организации)) КАК ТоварыОрганизацийОстатки
		|ГДЕ ТоварыОрганизацийОстатки.КоличествоОстаток&gt;0
		|
		|УПОРЯДОЧИТЬ ПО
		|	Приоритет УБЫВ,
		|	НомерГТД";
		
		//++БК3
		Если ЛОЖЬ Тогда
		//--БК3
			
			Запрос.Текст = СтрЗаменить(Запрос.Текст, "&amp;ХарактеристикаНоменклатуры", "ТоварыОрганизацийОстатки.ХарактеристикаНоменклатуры");
			Запрос.Текст = СтрЗаменить(Запрос.Текст, "&amp;СерияНоменклатуры", "ТоварыОрганизацийОстатки.СерияНоменклатуры");
			
		Иначе
			
			Запрос.Текст = СтрЗаменить(Запрос.Текст, "&amp;ХарактеристикаНоменклатуры", """");
			Запрос.Текст = СтрЗаменить(Запрос.Текст, "&amp;СерияНоменклатуры", """");
							
		КонецЕсли;
		Если Параметры.Организации.количество()=0 Тогда
			Запрос.Текст = СтрЗаменить(Запрос.Текст, "Организация В (&amp;Организации)", "");
		КонецЕсли;
		//	
		//Если Найти(Запрос.Текст, "ВТ_Остатки.Приоритет УБЫВ") = 0 Тогда
		//	ВызватьИсключение НСтр("ru = 'В тексте запроса отсутствует требуемая строка.'");
		//КонецЕсли;
		
		Если Параметры.какз_СначалаПустымиГТД Тогда		
			Запрос.Текст = СтрЗаменить(Запрос.Текст, "ВТ_Остатки.Приоритет УБЫВ", "ВТ_Остатки.Приоритет ВОЗР");  	
		//ИначеЕсли Параметры.СпособЗаполнения = НомераГТДКлиентСервер.СпособЗаполнения_СначалаЗаполненными() Тогда
		Иначе
			Запрос.Текст = СтрЗаменить(Запрос.Текст, "ВТ_Остатки.Приоритет УБЫВ", "ВТ_Остатки.Приоритет УБЫВ");  	
		КонецЕсли;
		//Иначе		
		//	ВызватьИсключение НСтр("ru = 'Необрабатываемое значение параметра ""СпособЗаполнения"".'");
		//КонецЕсли;
		
		Запрос.УстановитьПараметр("Организации", Параметры.Организации);
		Запрос.УстановитьПараметр("Период", Дата("20221231235959"));
			
		Параметры.ПараметрыАлгоритмов.ДанныеУчетаТоваровОрганизацийПоИсточникамПроисхождения = Запрос.Выполнить().Выгрузить();
		
	КонецЕсли;
	
	ОстаткиПоНомерамГТД = Параметры.ПараметрыАлгоритмов.ДанныеУчетаТоваровОрганизацийПоИсточникамПроисхождения;
	
	КопияТаблицаОстатков = ТаблицаОстатков.СкопироватьКолонки();
	
	Для Каждого СтрокаОстаток Из ТаблицаОстатков Цикл
		ПараметрыОтбора = Новый  Структура;
		ПараметрыОтбора.Вставить("Организация", СтрокаОстаток.Организация);
		ПараметрыОтбора.Вставить("Номенклатура", СтрокаОстаток.Номенклатура);
		//++БК3
		Если ЛОЖЬ Тогда
		//--БК3
			Если ТаблицаОстатков.Колонки.Найти("Характеристика") &lt;&gt; Неопределено Тогда
				ПараметрыОтбора.Вставить("ХарактеристикаНоменклатуры", СтрокаОстаток.Характеристика);
			КонецЕсли;
			Если ТаблицаОстатков.Колонки.Найти("ВестиУчетПоСериям") &lt;&gt; Неопределено И СтрокаОстаток.ВестиУчетПоСериям Тогда
				ПараметрыОтбора.Вставить("СерияНоменклатуры", СтрокаОстаток.Серия);
			КонецЕсли;
		КонецЕсли;
		Если ВедетсяУчетПоСкладам Тогда
			ПараметрыОтбора.Вставить("Склад", СтрокаОстаток.Склад);
		КонецЕсли; 
		
		МассивСтрокОстатковПоНомерамГТД = ОстаткиПоНомерамГТД.НайтиСтроки(ПараметрыОтбора);
		
		Для Каждого СтрокаОстатковГТД Из МассивСтрокОстатковПоНомерамГТД Цикл
			Если СтрокаОстатковГТД.КоличествоОстаток&gt;= СтрокаОстаток.Количество Тогда
				СтрокаОстатковГТД.КоличествоОстаток = СтрокаОстатковГТД.КоличествоОстаток - СтрокаОстаток.Количество;
				НоваяСтрока = КопияТаблицаОстатков.Добавить();
				ЗаполнитьЗначенияСвойств(НоваяСтрока,СтрокаОстаток);
				НоваяСтрока.НомерГТД = СтрокаОстатковГТД.НомерГТД;
				СтрокаОстаток.Количество = 0;
			Иначе
				НоваяСтрока = КопияТаблицаОстатков.Добавить();
				ЗаполнитьЗначенияСвойств(НоваяСтрока,СтрокаОстаток);
				НоваяСтрока.НомерГТД = СтрокаОстатковГТД.НомерГТД;
				НоваяСтрока.Количество = СтрокаОстатковГТД.КоличествоОстаток;
			
				НоваяСтрока.СуммаБезНДС = СтрокаОстаток.СуммаБезНДС * СтрокаОстатковГТД.КоличествоОстаток / СтрокаОстаток.Количество;
				НоваяСтрока.СуммаНДС = СтрокаОстаток.СуммаНДС * СтрокаОстатковГТД.КоличествоОстаток / СтрокаОстаток.Количество;
				НоваяСтрока.Сумма = СтрокаОстаток.Сумма * СтрокаОстатковГТД.КоличествоОстаток / СтрокаОстаток.Количество;
				НоваяСтрока.СуммаРегл = СтрокаОстаток.СуммаРегл * СтрокаОстатковГТД.КоличествоОстаток / СтрокаОстаток.Количество;
				НоваяСтрока.НДСРегл = СтрокаОстаток.НДСРегл * СтрокаОстатковГТД.КоличествоОстаток / СтрокаОстаток.Количество;
				
				// дальше уменьшаем в исходных таблицах значения (или удаляем строки)							
				СтрокаОстаток.Количество  = СтрокаОстаток.Количество - НоваяСтрока.Количество;
				СтрокаОстаток.СуммаБезНДС = СтрокаОстаток.СуммаБезНДС - НоваяСтрока.СуммаБезНДС; // Сумма, СуммаНДС - пустые в остатках упр. учета
				СтрокаОстаток.Сумма     = СтрокаОстаток.Сумма - НоваяСтрока.Сумма;
				СтрокаОстаток.СуммаНДС  = СтрокаОстаток.СуммаНДС - НоваяСтрока.СуммаНДС;
				СтрокаОстаток.СуммаРегл = СтрокаОстаток.СуммаРегл - НоваяСтрока.СуммаРегл;
				СтрокаОстаток.НДСРегл   = СтрокаОстаток.НДСРегл - НоваяСтрока.НДСРегл;
			КонецЕсли;
			Если СтрокаОстатковГТД.КоличествоОстаток=0 Тогда
				ОстаткиПоНомерамГТД.Удалить(СтрокаОстатковГТД);
			КонецЕсли;
			Если СтрокаОстаток.Количество = 0 Тогда 
				Прервать;
			КонецЕсли;
		КонецЦикла;
		Если СтрокаОстаток.Количество&gt;0 Тогда
			НоваяСтрока = КопияТаблицаОстатков.Добавить();
			ЗаполнитьЗначенияСвойств(НоваяСтрока,СтрокаОстаток);
		КонецЕсли;
			
	КонецЦикла;
	
	ТаблицаОстатков = КопияТаблицаОстатков;
	
		
	КонецЕсли;</Текст>
				</Алгоритм>
				<Алгоритм Имя="ОпределитьГруппуФинУчетаПоСчетуУчета" ИспользуетсяПриЗагрузке="false">
					<Текст>//++БК3
//Если СчетУчета = ПланыСчетов.Типовой.ТоварыПринятыеНаКомиссию 
//--БК3	
	Если СчетУчета = ПланыСчетов.Типовой.Товары  Тогда
	ГруппаФинансовогоУчета = "Товары на складах";
Иначе
	СвойстваСчетаУчета = ОбщегоНазначения.ПолучитьЗначенияРеквизитов(СчетУчета, "Код, Наименование");
	ГруппаФинансовогоУчета = СвойстваСчетаУчета.Наименование + " (" + СвойстваСчетаУчета.Код + ")";
КонецЕсли</Текст>
				</Алгоритм>
				<Алгоритм Имя="ПосчитатьРазницуДобавитьВоВременные" ИспользуетсяПриЗагрузке="false">
					<Текст>НаименованиеТекущегоПВД = "";
Если ТипОперации = "ОстаткиСобственныхТоваров" Тогда
	НаименованиеТекущегоПВД = НСтр("ru='Собственные товары на складах'");
ИначеЕсли ТипОперации = "ОстаткиТоваровПолученныхНаКомиссию" Тогда 
	НаименованиеТекущегоПВД = НСтр("ru='Товары, принятые на комиссию'");
ИначеЕсли ТипОперации = "ОстаткиТоваровПереданныхНаКомиссию" Тогда 
	НаименованиеТекущегоПВД = НСтр("ru='Товары, переданные на комиссию'");
ИначеЕсли ТипОперации = "ОстаткиВозвратнойТарыПринятойОтПоставщиков" Тогда
	НаименованиеТекущегоПВД = НСтр("ru='Возвратная тара, полученная от поставщика'");
ИначеЕсли ТипОперации = "ОстаткиВозвратнойТарыПереданнойКлиентам" Тогда
	НаименованиеТекущегоПВД = НСтр("ru='Возвратная тара, переданная клиенту'");
ИначеЕсли ТипОперации = "ОстаткиМатериаловПереданныхПереработчикам" Тогда	
	НаименованиеТекущегоПВД = НСтр("ru='Материалы, переданные в переработку'");
ИначеЕсли ТипОперации = "ОстаткиМатериаловПереданныхВПроизводство" Тогда	
	НаименованиеТекущегоПВД = НСтр("ru='Материалы, переданные в производство'");
КонецЕсли;

Если ТаблицаУпр.Колонки.Найти("Склад") = Неопределено Тогда
	ТаблицаУпр.Колонки.Добавить("Склад");
КонецЕсли;
Если ТаблицаРегл.Колонки.Найти("Склад") = Неопределено Тогда
	ТаблицаРегл.Колонки.Добавить("Склад");
КонецЕсли;
Если ТаблицаУпр.Колонки.Найти("Договор") = Неопределено Тогда
	ТаблицаУпр.Колонки.Добавить("Договор");
КонецЕсли;
Если ТаблицаРегл.Колонки.Найти("Договор") = Неопределено Тогда
	ТаблицаРегл.Колонки.Добавить("Договор");
КонецЕсли;
Если ТаблицаРегл.Колонки.Найти("СложныйУчетНДС") = Неопределено Тогда
	ТаблицаРегл.Колонки.Добавить("СложныйУчетНДС");
КонецЕсли;


// из таблицы упр заранее удаляем отрицательные остатки. т.к. сравнение с ними будет некорректным
ИндексУпр = ТаблицаУпр.Количество()-1;
Пока ИндексУпр &gt;= 0 Цикл
	СтрУпр = ТаблицаУпр.Получить(ИндексУпр);
	Если Окр(СтрУпр.Количество, 2) &lt; 0 Или Окр(СтрУпр.СуммаБезНДС, 2) &lt; 0 Тогда		
		СтрокаСообщения = "";				
		Если Окр(СтрУпр.Количество, 2) &lt;= 0.00 Тогда
			СтрокаСообщения = НСтр("ru='Обнаружен отрицательный остаток на складе ""%1"" по номенклатуре ""%2"" при выгрузке по правилу: ""%3"".'");
		ИначеЕсли Окр(СтрУпр.СуммаБезНДС, 2) &lt;= 0 Тогда
			СтрокаСообщения = НСтр("ru='Обнаружен отрицательный суммовой остаток на складе ""%1"" по товару ""%2"" при выгрузке по правилу: ""%3"".'");
		КонецЕсли;
		
		Если ЗначениеЗаполнено(СтрокаСообщения) Тогда
			СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%1", СтрУпр.Склад);
			СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%2", СтрУпр.Номенклатура);
			СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%3", НаименованиеТекущегоПВД);
			
			ЗаписатьВПротоколВыполнения(СтрокаСообщения,,Ложь);
		КонецЕсли;
		Сообщить("АЛГОРИТМ ПосчитатьРазницуДобавитьВоВременные условие 1: " + СтрокаСообщения) ;
		ТаблицаУпр.Удалить(СтрУпр);
	КонецЕсли;	
	ИндексУпр = ИндексУпр - 1;
КонецЦикла;

Если ТаблицаУпр.Колонки.Найти("Организация") = Неопределено Тогда
	ТаблицаУпр.Колонки.Добавить("Организация");
	Для Каждого СтрокаУпр Из ТаблицаУпр Цикл
		СтрокаУпр.Организация = Справочники.Организации.ПустаяСсылка();
	КонецЦикла;
КонецЕсли;
Параметры.ПараметрыАлгоритмов.ПризнакиВеденияУчета.ВедетсяУчетРеглХарактеристики = Ложь;
Параметры.ПараметрыАлгоритмов.ПризнакиВеденияУчета.ВедетсяУчетРеглСерии          = Ложь;
Параметры.ПараметрыАлгоритмов.ПризнакиВеденияУчета.ВедетсяУчетУпрОрганизации     = Ложь;
Параметры.ПараметрыАлгоритмов.ПризнакиВеденияУчета.ВедетсяУчетУпрСчетУчета       = Ложь;
Для Каждого СтрокаРегл Из ТаблицаРегл Цикл
	Если ЗначениеЗаполнено(СтрокаРегл.Характеристика) Тогда
		Параметры.ПараметрыАлгоритмов.ПризнакиВеденияУчета.ВедетсяУчетРеглХарактеристики = Истина;
	КонецЕсли;
	Если ЗначениеЗаполнено(СтрокаРегл.Серия) Тогда
		Параметры.ПараметрыАлгоритмов.ПризнакиВеденияУчета.ВедетсяУчетРеглСерии = Истина;			
	КонецЕсли;		
КонецЦикла;
Для Каждого СтрокаУпр Из ТаблицаУпр Цикл
	Если ЗначениеЗаполнено(СтрокаУпр.Организация) Тогда
		Параметры.ПараметрыАлгоритмов.ПризнакиВеденияУчета.ВедетсяУчетУпрОрганизации = Истина;
	КонецЕсли;
	Если ТаблицаУпр.Колонки.Найти("СчетУчета")&lt;&gt;Неопределено И ЗначениеЗаполнено(СтрокаУпр.СчетУчета) Тогда
		Параметры.ПараметрыАлгоритмов.ПризнакиВеденияУчета.ВедетсяУчетУпрСчетУчета = Истина;
	КонецЕсли;		
	Если Не Параметры.ПараметрыАлгоритмов.ПризнакиВеденияУчета.ВедетсяУчетРеглХарактеристики Тогда
		//++БК3
		СтрокаУпр.Характеристика = Null;
		//--БК3
		СтрокаУпр.Качество = "Новый";
	КонецЕсли;
	Если Не Параметры.ПараметрыАлгоритмов.ПризнакиВеденияУчета.ВедетсяУчетРеглСерии Тогда
		//++БК3
		СтрокаУпр.Серия = Null;
		//--БК3
	КонецЕсли;		
КонецЦикла;

Если ТаблицаУпр.Колонки.Найти("СтатусПартии")&lt;&gt;Неопределено Тогда
	ТаблицаУпр.Индексы.Добавить("Организация, Склад, Номенклатура, СтатусПартии");
Иначе
	ТаблицаУпр.Индексы.Добавить("Организация, Склад, Номенклатура");
КонецЕсли;	
	
ТаблицаРеглРез = Новый ТаблицаЗначений;
ТаблицаРеглРез = ТаблицаРегл.СкопироватьКолонки();

Если ТаблицаРеглРез.Колонки.Найти("СтавкаНДС") = Неопределено Тогда
	ТаблицаРеглРез.Колонки.Добавить("СтавкаНДС");
КонецЕсли;	
Если ТаблицаРеглРез.Колонки.Найти("Характеристика") = Неопределено Тогда
	//++БК3
	ТаблицаРеглРез.Колонки.Добавить("Характеристика");
	//--БК3
КонецЕсли;
Если ТаблицаРеглРез.Колонки.Найти("Серия") = Неопределено Тогда
	//++БК3
	ТаблицаРеглРез.Колонки.Добавить("Серия");
	//--БК3
КонецЕсли;
Если ТаблицаРеглРез.Колонки.Найти("Качество") = Неопределено Тогда
	//++БК3
	ТаблицаРеглРез.Колонки.Добавить("Качество");
	//--БК3
КонецЕсли;
Если ЗаполнятьТаблицуНесопоставленных Тогда
	ТаблицаНесопоставленных = ТаблицаРеглРез.СкопироватьКолонки();
КонецЕсли;

СкладОстатков        = Неопределено;
НоменклатураОстатков = Неопределено;
ОрганизацияОстатков  = Неопределено;

ИндексРегл = ТаблицаРегл.Количество() - 1;
Пока ИндексРегл &gt;= 0 Цикл
	СтрокаРегл = ТаблицаРегл.Получить(ИндексРегл);
	Если ТаблицаРегл.Колонки.Найти("Количество1") &lt;&gt; Неопределено И Окр(СтрокаРегл.Количество1, 0) = СтрокаРегл.Количество1 Тогда
		СтрокаРегл.Количество = Окр(СтрокаРегл.Количество, 0);
	КонецЕсли;
	
	Если Не ЗначениеЗаполнено(СтрокаРегл.Номенклатура) Или Не ЗначениеЗаполнено(СтрокаРегл.Количество) Тогда
		ТаблицаРегл.Удалить(СтрокаРегл);
		ИндексРегл = ИндексРегл - 1;
		Продолжить;
	КонецЕсли;		
		
	Если СкладОстатков &lt;&gt; СтрокаРегл.Склад Или НоменклатураОстатков &lt;&gt; СтрокаРегл.Номенклатура Или ОрганизацияОстатков &lt;&gt; СтрокаРегл.Организация  Тогда
		
		СкладОстатков                 = СтрокаРегл.Склад;
		НоменклатураОстатков          = СтрокаРегл.Номенклатура;
		ОрганизацияОстатков           = СтрокаРегл.Организация;
		
		// здесь поиск по складу и номенклатуре в таблице упр.
		// сначала поиск без организации, т.к. выбирать партии начинаем снизу в таблице Упр
		СтруктураОтбораБезОрганизации = Новый Структура;
		СтруктураОтбораБезОрганизации.Вставить("Склад", СкладОстатков);
		СтруктураОтбораБезОрганизации.Вставить("Номенклатура", НоменклатураОстатков);
		СтруктураОтбораБезОрганизации.Вставить("Организация", Справочники.Организации.ПустаяСсылка());
		Если Параметры.ПараметрыАлгоритмов.ПризнакиВеденияУчета.ВедетсяУчетРеглХарактеристики Тогда
			СтруктураОтбораБезОрганизации.Вставить("Характеристика", СтрокаРегл.Характеристика);
		КонецЕсли;
		Если Параметры.ПараметрыАлгоритмов.ПризнакиВеденияУчета.ВедетсяУчетРеглСерии Тогда
			СтруктураОтбораБезОрганизации.Вставить("Серия", СтрокаРегл.Серия);
		КонецЕсли;
		Если Параметры.ПараметрыАлгоритмов.ПризнакиВеденияУчета.ВедетсяУчетУпрСчетУчета Тогда
			СтруктураОтбораБезОрганизации.Вставить("СчетУчета", СтрокаРегл.Серия);
		КонецЕсли;			
		
		Если Параметры.ПараметрыАлгоритмов.ИмяТекущегоМассиваОстатков = "МассивПринятыхНаКомиссиюОстатковТоваров" Тогда
			СтруктураОтбораБезОрганизации.Вставить("СтатусПартии", Перечисления.СтатусыПартийТоваров.НаКомиссию);
		КонецЕсли;
			
		СтрокиУпр = ТаблицаУпр.НайтиСтроки(СтруктураОтбораБезОрганизации);
		
		Если Параметры.ПараметрыАлгоритмов.ПризнакиВеденияУчета.ВедетсяУчетУпрОрганизации Тогда
			СтруктураОтбора = Новый Структура;
			СтруктураОтбора.Вставить("Склад",        СкладОстатков);
			СтруктураОтбора.Вставить("Номенклатура", НоменклатураОстатков);
			СтруктураОтбора.Вставить("Организация",  ОрганизацияОстатков);		
			Если Параметры.ПараметрыАлгоритмов.ИмяТекущегоМассиваОстатков = "МассивПринятыхНаКомиссиюОстатковТоваров" Тогда
				СтруктураОтбора.Вставить("СтатусПартии", Перечисления.СтатусыПартийТоваров.НаКомиссию);
			КонецЕсли;		
			
			СтрокиУпрСОрганизацией = ТаблицаУпр.НайтиСтроки(СтруктураОтбора);
					
			Для Каждого СтрокаСОрганизацией Из СтрокиУпрСОрганизацией Цикл
				СтрокиУпр.Добавить(СтрокаСОрганизацией);
			КонецЦикла;	
		КонецЕсли;
		
		ИндексУпр = СтрокиУпр.Количество()-1;
	КонецЕсли;
	
	Если Параметры.ПараметрыАлгоритмов.ИмяТекущегоМассиваОстатков &lt;&gt; "МассивПереданныхНаКомиссиюОстатковТоваров" 
		И (СтрокаРегл.Количество &lt;= 0 Или Не ЗначениеЗаполнено(СтрокаРегл.Сумма) Или СтрокаРегл.Сумма &lt; 0
		 Или Не ЗначениеЗаполнено(СтрокаРегл.СуммаРегл) Или СтрокаРегл.СуммаРегл &lt; 0) Тогда
		 
		СтрокаСообщения = "";				
		Если Окр(СтрокаРегл.Количество, 2) &lt;= 0 Тогда
			СтрокаСообщения = НСтр("ru='Обнаружен отрицательный количественный остаток на складе ""%1"" по номенклатуре ""%2"" при выгрузке по правилу: ""%3"".'");
		Иначе
			СтрокаСообщения = НСтр("ru='Обнаружен некорректный суммовой остаток на складе ""%1"" по товару ""%2"" при выгрузке по правилу: ""%3"".'");
		КонецЕсли;
		
		Если ЗначениеЗаполнено(СтрокаСообщения) Тогда
			СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%1", СтрокаРегл.Склад);
			СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%2", СтрокаРегл.Номенклатура);
			СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%3", НаименованиеТекущегоПВД);
			
			ЗаписатьВПротоколВыполнения(СтрокаСообщения,,Ложь);
		КонецЕсли;
		Сообщить("АЛГОРИТМ ПосчитатьРазницуДобавитьВоВременные условие 2: " + СтрокаСообщения) ;
		ТаблицаРегл.Удалить(СтрокаРегл);
		ИндексРегл = ИндексРегл - 1;
	ИначеЕсли ИндексУпр = -1 Или (ТаблицаРеглРез.Колонки.Найти("УжеСопоставленоСУпр")&lt;&gt;Неопределено И СтрокаРегл.УжеСопоставленоСУпр) Тогда
		// УжеСопоставленоСУпр - означает, что ранее в алгоритме эти данные уже сопоставили с данными упр. Поэтому их надо просто добавить в выборку.
		
		Если Не ЗаполнятьТаблицуНесопоставленных Тогда
			НоваяСтрока = ТаблицаРеглРез.Добавить();
		Иначе
			НоваяСтрока = ТаблицаНесопоставленных.Добавить();
		КонецЕсли;
		
		ЗаполнитьЗначенияСвойств(НоваяСтрока, СтрокаРегл);	
		//Сообщить("АЛГОРИТМ ПосчитатьРазницуДобавитьВоВременные условие 1: " + СтрокаСообщения) ;
		ТаблицаРегл.Удалить(СтрокаРегл);
		ИндексРегл = ИндексРегл - 1;
		
	Иначе
		
		СтрокаУпр = СтрокиУпр.Получить(ИндексУпр);
		Если СтрокаУпр.Количество &lt;= 0 Или Не ЗначениеЗаполнено(СтрокаУпр.Сумма) Или СтрокаУпр.Сумма &lt;= 0 Тогда
			ИндексУпр = ИндексУпр - 1;
			Продолжить;
		КонецЕсли;
		
		НоваяСтрока = ТаблицаРеглРез.Добавить();
		ЗаполнитьЗначенияСвойств(НоваяСтрока, СтрокаУпр);
		ЗаполнитьЗначенияСвойств(НоваяСтрока, СтрокаРегл); // Количество, СуммаРегл, НДСРегл - корректные установились
		Если Не ЗначениеЗаполнено(НоваяСтрока.СтавкаНДС) И ЗначениеЗаполнено(СтрокаРегл.СтавкаНДС) Тогда
			НоваяСтрока.СтавкаНДС = СтрокаРегл.СтавкаНДС;
		КонецЕсли;
		Если ЗначениеЗаполнено(СтрокаУпр.Договор) И Не ЗначениеЗаполнено(СтрокаРегл.Договор) Тогда
			НоваяСтрока.Договор = СтрокаУпр.Договор;
		КонецЕсли;
		
		//++БК3
		СтавкаНДСЧислом = УчетНДСиАкцизаВызовСервераПовтИсп.ПолучитьСтавкуНДС(НоваяСтрока.СтавкаНДС) / 100;
		//--БК3
		
		Если СтрокаУпр.Количество &gt;= СтрокаРегл.Количество Тогда
			Если Не Параметры.ПараметрыАлгоритмов.ПризнакиВеденияУчета.ВедетсяУчетРеглХарактеристики Тогда
				//++БК3
				НоваяСтрока.Характеристика = Null;
				//--БК3
			КонецЕсли;
			Если Не Параметры.ПараметрыАлгоритмов.ПризнакиВеденияУчета.ВедетсяУчетРеглСерии Тогда
				//++БК3
				НоваяСтрока.Серия = Null;
				//--БК3
			КонецЕсли;
			
			Если Не Параметры.ИспользоватьРАУЗ Тогда // СтрокаУпр.СуммаБезНДС если не РАУЗ содержит НДС на самом деле
				СтрокаУпр.СуммаБезНДС = СтрокаУпр.СуммаБезНДС / (1 + СтавкаНДСЧислом);
			КонецЕсли;
			
			НоваяСтрока.СуммаБезНДС = СтрокаУпр.СуммаБезНДС * СтрокаРегл.Количество / СтрокаУпр.Количество;
			НоваяСтрока.СуммаНДС    = НоваяСтрока.СуммаБезНДС * СтавкаНДСЧислом;
			НоваяСтрока.Сумма       = НоваяСтрока.СуммаНДС + НоваяСтрока.СуммаБезНДС;
			Если Параметры.ПараметрыАлгоритмов.ИмяТекущегоМассиваОстатков = "МассивПереданныхНаКомиссиюОстатковТоваров" Тогда
				НоваяСтрока.СуммаРегл = СтрокаУпр.СуммаРегл * СтрокаРегл.Количество / СтрокаУпр.Количество;
				НоваяСтрока.НДСРегл = СтрокаУпр.НДСРегл * СтрокаРегл.Количество / СтрокаУпр.Количество;
			КонецЕсли;				
			
			// дальше уменьшаем в исходных таблицах значения (или удаляем строки)							
			СтрокаУпр.Количество  = СтрокаУпр.Количество - НоваяСтрока.Количество;
			СтрокаУпр.СуммаБезНДС = СтрокаУпр.СуммаБезНДС - НоваяСтрока.СуммаБезНДС; // Сумма, СуммаНДС - пустые в остатках упр. учета
			Если Параметры.ПараметрыАлгоритмов.ИмяТекущегоМассиваОстатков = "МассивПереданныхНаКомиссиюОстатковТоваров" Тогда
				СтрокаУпр.Сумма     = СтрокаУпр.Сумма - НоваяСтрока.Сумма;
				СтрокаУпр.СуммаНДС  = СтрокаУпр.СуммаНДС - НоваяСтрока.СуммаНДС;
				СтрокаУпр.СуммаРегл = СтрокаУпр.СуммаРегл - НоваяСтрока.СуммаРегл;
				СтрокаУпр.НДСРегл   = СтрокаУпр.НДСРегл - НоваяСтрока.НДСРегл;
			КонецЕсли;
			
			ТаблицаРегл.Удалить(СтрокаРегл);
			ИндексРегл = ИндексРегл - 1;
			
			Если СтрокаУпр.Количество = 0 Тогда
				ТаблицаУпр.Удалить(СтрокаУпр);
				ИндексУпр = ИндексУпр - 1;
			КонецЕсли;
			
		Иначе // если количество регл. учета больше
			НоваяСтрока.Количество   = СтрокаУпр.Количество; // сначала берем тут количество упр. учета - это нормально.
			// Далее все равно несопоставленные остатки регл. учета добавим.
			НоваяСтрока.СуммаБезНДС = СтрокаУпр.СуммаБезНДС;
			НоваяСтрока.СуммаНДС    = НоваяСтрока.СуммаБезНДС * СтавкаНДСЧислом;
			НоваяСтрока.Сумма       = НоваяСтрока.СуммаНДС + НоваяСтрока.СуммаБезНДС;
			Если Параметры.ПараметрыАлгоритмов.ИмяТекущегоМассиваОстатков &lt;&gt; "МассивПереданныхНаКомиссиюОстатковТоваров" Тогда
				НоваяСтрока.СуммаРегл   = СтрокаРегл.СуммаРегл * СтрокаУпр.Количество / СтрокаРегл.Количество;
				НоваяСтрока.НДСРегл     = СтрокаРегл.НДСРегл * СтрокаУпр.Количество / СтрокаРегл.Количество;					
				
				СтрокаРегл.СуммаРегл    = СтрокаРегл.СуммаРегл - НоваяСтрока.СуммаРегл;
				СтрокаРегл.НДСРегл      = СтрокаРегл.НДСРегл - НоваяСтрока.НДСРегл;				
			Иначе
				ЗаполнитьЗначенияСвойств(НоваяСтрока, СтрокаУпр);
			КонецЕсли;
			
			СтрокаРегл.Количество   = СтрокаРегл.Количество - СтрокаУпр.Количество;
			
			ТаблицаУпр.Удалить(СтрокаУпр);
			ИндексУпр = ИндексУпр - 1;	
		КонецЕсли;			
			
	КонецЕсли;
	
	Если НоваяСтрока &lt;&gt; Неопределено И Не ЗначениеЗаполнено(НоваяСтрока.СтавкаНДС) Тогда
		НоваяСтрока.СтавкаНДС = НоваяСтрока.Номенклатура.СтавкаНДС;	
	КонецЕсли;	
	
КонецЦикла;

СтрокиГруппировок = "";
СтрокиСуммирования = "";
Для Каждого Колонка Из ТаблицаРеглРез.Колонки Цикл
	Имя = Колонка.Имя;
	Если Имя = "Количество" Или Имя = "Количество1" Или Имя = "Сумма" Или Имя = "НДСРегл" Или Имя = "СуммаБезНДС" Или Имя = "СуммаНДС" Или Имя = "СуммаРегл" Тогда
		Если СтрокиСуммирования &lt;&gt; "" Тогда
			СтрокиСуммирования = СтрокиСуммирования + ",";
		КонецЕсли;
		СтрокиСуммирования = СтрокиСуммирования + Имя;
	Иначе
		Если СтрокиГруппировок &lt;&gt; "" Тогда
			СтрокиГруппировок = СтрокиГруппировок + ",";
		КонецЕсли;
		СтрокиГруппировок = СтрокиГруппировок + Имя;
	КонецЕсли;	
КонецЦикла;                                                

ТаблицаРеглРез.Свернуть(СтрокиГруппировок, СтрокиСуммирования);

Параметры.ПараметрыАлгоритмов.ТаблицаУпрВрем  = ТаблицаУпр;
Параметры.ПараметрыАлгоритмов.ТаблицаРеглВрем = ТаблицаРеглРез;

Если ЗаполнятьТаблицуНесопоставленных Тогда
	Параметры.ПараметрыАлгоритмов.ТаблицаРеглНесопоставленоВрем = ТаблицаНесопоставленных;
КонецЕсли;</Текст>
					<Параметры>ТаблицаУпр, ТаблицаРегл, ЗаполнятьТаблицуНесопоставленных</Параметры>
				</Алгоритм>
				<Алгоритм Имя="ТМЦВЭксплуатацииОбработатьВыборкуДанных" ИспользуетсяПриЗагрузке="false">
					<Текст>ВыборкаДанных = Новый ТаблицаЗначений;
ВыборкаДанных.Колонки.Добавить("Подразделение");
ВыборкаДанных.Колонки.Добавить("Дата");
ВыборкаДанных.Колонки.Добавить("Организация");
ВыборкаДанных.Колонки.Добавить("ТМЦВЭксплуатации");
ВыборкаДанных.Колонки.Добавить("Комментарий");

ОрганизацияДокумента    = Неопределено;
ПодразделениеДокумента  = Неопределено;
НоваяСтрока             = Неопределено;

Для Каждого Строка Из ТаблицаОстатков Цикл
	Попытка
	Если ОрганизацияДокумента &lt;&gt; Строка.Организация Или ПодразделениеДокумента &lt;&gt; Строка.Подразделение Тогда
		Если НоваяСтрока &lt;&gt; Неопределено И (ОрганизацияДокумента &lt;&gt; Неопределено Или ПодразделениеДокумента &lt;&gt; Неопределено) Тогда
			Если Не ЗначениеЗаполнено(НоваяСтрока.ТМЦВЭксплуатации) Или НоваяСтрока.ТМЦВЭксплуатации.Количество() = 0 Тогда
				ВыборкаДанных.Удалить(НоваяСтрока);
			КонецЕсли;
		КонецЕсли;		
		ОрганизацияДокумента    = Строка.Организация;
		ПодразделениеДокумента  = Строка.Подразделение;
		НоваяСтрока = ВыборкаДанных.Добавить();
		НоваяСтрока.Дата          = КонецДня(Дата("20221231235959")); 
		НоваяСтрока.Организация   = Строка.Организация;
		НоваяСтрока.Подразделение = Строка.Подразделение;
		НаименованиеТекущегоПВД = НСтр("ru='ТМЦ, переданные в эксплуатацию'");
		ТекстКомментария = "";
		Комментарий      = "";
		Выполнить(Алгоритмы.ПредставлениеКомментария);

		НоваяСтрока.Комментарий = Комментарий + " %1";
		НоваяСтрока.Комментарий = СтрЗаменить(НоваяСтрока.Комментарий, "%1", НаименованиеТекущегоПВД);
		ДополнениеККомментарию = "";
		Если Не ЗначениеЗаполнено(Строка.Организация) Тогда
			ДополнениеККомментарию = ДополнениеККомментарию + " " + НСтр("ru='Эти данные будут введены по управленческой организации'") + ".";
		КонецЕсли;
			
		НоваяСтрока.Комментарий = НоваяСтрока.Комментарий + ДополнениеККомментарию;
		НоваяСтрока.ТМЦВЭксплуатации = Новый ТаблицаЗначений;
		НоваяСтрока.ТМЦВЭксплуатации.Колонки.Добавить("Номенклатура");
		НоваяСтрока.ТМЦВЭксплуатации.Колонки.Добавить("Характеристика");
		НоваяСтрока.ТМЦВЭксплуатации.Колонки.Добавить("СчетУчета");
		НоваяСтрока.ТМЦВЭксплуатации.Колонки.Добавить("ФизическоеЛицо");
		НоваяСтрока.ТМЦВЭксплуатации.Колонки.Добавить("ДатаПередачиВЭксплуатацию");
		НоваяСтрока.ТМЦВЭксплуатации.Колонки.Добавить("НаименованиеКатегории");
		НоваяСтрока.ТМЦВЭксплуатации.Колонки.Добавить("СпособПогашенияСтоимости");
		НоваяСтрока.ТМЦВЭксплуатации.Колонки.Добавить("СпособОтраженияРасходов");
		НоваяСтрока.ТМЦВЭксплуатации.Колонки.Добавить("СрокЭксплуатации");
		НоваяСтрока.ТМЦВЭксплуатации.Колонки.Добавить("Количество");
		НоваяСтрока.ТМЦВЭксплуатации.Колонки.Добавить("ПервоначальнаяСумма");
		НоваяСтрока.ТМЦВЭксплуатации.Колонки.Добавить("Сумма");

	КонецЕсли;
	Если Окр(Строка.Количество, 2) = 0.00 Тогда
		Продолжить;
	КонецЕсли;
	СтрокаСообщения = "";
	Если Окр(Строка.Количество, 2) &lt; 0.00 Тогда
		СтрокаСообщения = НСтр("ru='Обнаружен отрицательный остаток подразделение ""%1"" по ТМЦ ""%2"" физ лицо ""%7"" при выгрузке по правилу: ""%3"" %6'");
	ИначеЕсли Не ЗначениеЗаполнено(Строка.Подразделение)  Тогда
		СтрокаСообщения = НСтр("ru='Обнаружено пустое значение подразделения для ТМЦ ""%2"" физ лицо ""%7"" организации ""%4"" с остатком ""%5"" шт. при выгрузке по правилу: ""%3""'");
	КонецЕсли;
	
	Если ЗначениеЗаполнено(СтрокаСообщения) Тогда
		СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%1", Строка.Подразделение);
		СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%2", Строка.Номенклатура);
		СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%3", НаименованиеТекущегоПВД);
		СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%4", Строка.Организация);
		СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%5", Строка.Количество);
		СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%7", Строка.ФизическоеЛицо);

		Если ЗначениеЗаполнено(Строка.Организация) Тогда
			СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%6", НСтр("ru='по организации '") + Строка.Организация);
		Иначе
			СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%6", НСтр("ru='по управленческим данным.'"));
		КонецЕсли;
		
		ЗаписатьВПротоколВыполнения(СтрокаСообщения,,Ложь);
		Продолжить;
	КонецЕсли;
	НоваяСтрокаТовары = НоваяСтрока.ТМЦВЭксплуатации.Добавить();
	ЗаполнитьЗначенияСвойств(НоваяСтрокаТовары, Строка);
	Если Окр(НоваяСтрокаТовары.Сумма,2) &lt; 0.00 Тогда
		НоваяСтрокаТовары.Сумма = 0;
	КонецЕсли;
	Исключение

		ТекстОшибки = ОписаниеОшибки();
		Если ТекстОшибки &lt;&gt; "" Тогда
			ЗаписатьВПротоколВыполнения(ТекстОшибки,,Ложь);
		КонецЕсли;		
	КонецПопытки;

КонецЦикла;</Текст>
				</Алгоритм>
				<Алгоритм Имя="утп_ПолеДокумента" ИспользуетсяПриЗагрузке="false">
					<Текст>Если НовыйДокументОприходования = Неопределено ИЛИ НовыйДокументОприходования= Null ИЛИ НЕ ЗначениеЗаполнено(НовыйДокументОприходования) Тогда
		ПолеДокумента = ЗначениеПоляПоУмолчанию;
	Иначе	
		Попытка
			Если Поле="Дата" ИЛИ поле = "Номер" Тогда
				ПолеДокумента = НовыйДокументОприходования[Поле];
			ИначеЕсли НовыйДокументОприходования.Метаданные().Реквизиты.Найти(Поле)&lt;&gt;Неопределено Тогда
				ПолеДокумента = НовыйДокументОприходования[Поле];
			Иначе
				ПолеДокумента = ЗначениеПоляПоУмолчанию;
			КонецЕсли;
		Исключение
			ПолеДокумента = ЗначениеПоляПоУмолчанию;
		КонецПопытки;
	КонецЕсли;</Текст>
					<Параметры>ПолеДокумента,НовыйДокументОприходования,Поле,ЗначениеПоляПоУмолчанию</Параметры>
				</Алгоритм>
			</Группа>
			<Алгоритм Имя="ВходящиеДанныеНоменклатуры" ИспользуетсяПриЗагрузке="false">
				<Текст>Если Не ЗначениеЗаполнено(ВходящиеДанные) Тогда
	ВходящиеДанные = Новый Структура;
КонецЕсли;

// Тип номенклатуры
ВходящиеДанные.Вставить("ТипНоменклатуры");

Если Не Источник.ЭтоГруппа Тогда
	//++БК3
	//Если Параметры.ТабНоменклатурыТары.Найти(Источник.Ссылка, "Номенклатура") = Неопределено Тогда
		ВходящиеДанные.ТипНоменклатуры = ?(ОбщегоНазначения.ПолучитьЗначениеРеквизита(Источник, "Услуга"),"Услуга","Товар");
	//Иначе
	//	ВходящиеДанные.ТипНоменклатуры = "МногооборотнаяТара";
	//КонецЕсли;
	//--БК3
КонецЕсли;

// Качество
КлючКачествоСуществует = ВходящиеДанные.Свойство("Качество");
//++КАКЗ
Если Не КлючКачествоСуществует ИЛИ ВходящиеДанные.Качество=Null ИЛИ НЕ ЗначениеЗаполнено(ВходящиеДанные.Качество) Тогда
//--КАКЗ
	ВходящиеДанные.Вставить("Качество", "Новый");
ИначеЕсли СокрЛП(ВходящиеДанные.Качество) &lt;&gt; "Новый" Тогда
	ВходящиеДанные.Качество = "ОграниченноГоден";
Иначе
	ВходящиеДанные.Качество = "Новый";
КонецЕсли;

// Наименование
КлючНаименованиеСуществует = ВходящиеДанные.Свойство("Наименование");
Если Не КлючНаименованиеСуществует Тогда
	Если ВходящиеДанные.Качество &lt;&gt; "Новый" Тогда
		СтрокаБрак = НСтр("ru='Брак'");
		ВходящиеДанные.Вставить("Наименование", Лев(СокрЛП(Источник.Наименование + " (" + СтрокаБрак + ")"), 100));
	Иначе
		ВходящиеДанные.Вставить("Наименование", Лев(СокрЛП(Источник.Наименование), 100));
	КонецЕсли;
Иначе
	Если ВходящиеДанные.Качество &lt;&gt; "Новый" Тогда
		СтрокаБрак = НСтр("ru='Брак'");
		ВходящиеДанные.Наименование = Лев(СокрЛП(Источник.Наименование + " (" + СтрокаБрак + ")"), 100);
	Иначе
		ВходящиеДанные.Наименование = Лев(СокрЛП(Источник.Наименование), 100);
	КонецЕсли;
КонецЕсли;

// ГФУ
КлючГруппаФинУчетаСуществует = ВходящиеДанные.Свойство("ГруппаФинансовогоУчета");
//++КАКЗ
КлючГруппаФинУчетаСуществует = КлючГруппаФинУчетаСуществует И ЗначениеЗаполнено(ВходящиеДанные.ГруппаФинансовогоУчета);
//--КАКЗ

Если Не КлючГруппаФинУчетаСуществует Тогда
	// тогда ищем среди уже загруженных позиций номенклатуры
	ПараметрыПоиска = Новый Структура("Номенклатура", Источник.Ссылка);
		
	НайденныеСтроки = Параметры.ПараметрыАлгоритмов.НоменклатураПоГФУ.НайтиСтроки(ПараметрыПоиска);
	Если НайденныеСтроки.Количество() &gt; 0 Тогда
		ВходящиеДанные.Вставить("ГруппаФинансовогоУчета", НайденныеСтроки[0].ГФУ);
		ВходящиеДанные.Вставить("СчетУчетаНаСкладе", НайденныеСтроки[0].СчетУчетаНаСкладе);

	Иначе
		ВходящиеДанные.Вставить("ГруппаФинансовогоУчета", "");
		ВходящиеДанные.Вставить("СчетУчетаНаСкладе", "");
		//++КАКЗ
		ВходящиеДанные.Вставить("ГруппаФинансовогоУчета", ПланыСчетов.Типовой.Товары.Наименование+ "("+ПланыСчетов.Типовой.Товары.Код+")");
		ВходящиеДанные.Вставить("СчетУчетаНаСкладе", ПланыСчетов.Типовой.Товары);
		//--КАКЗ
	КонецЕсли;
//++КАКЗ
ИначеЕсли НЕ ВходящиеДанные.Свойство("СчетУчетаНаСкладе") Тогда
	ПараметрыПоиска = Новый Структура("Номенклатура", Источник.Ссылка);
		
	НайденныеСтроки = Параметры.ПараметрыАлгоритмов.НоменклатураПоГФУ.НайтиСтроки(ПараметрыПоиска);
	Если НайденныеСтроки.Количество() &gt; 0 Тогда
		ТребуемаяГФУ = НайденныеСтроки[0].ГФУ;
		ВходящиеДанные.Вставить("ГруппаФинансовогоУчета", ТребуемаяГФУ);
		ВходящиеДанные.Вставить("СчетУчетаНаСкладе", НайденныеСтроки[0].СчетУчетаНаСкладе);
	КонецЕсли;

//--КАКЗ
КонецЕсли;


//ПараметрыПоиска = Новый Структура("Номенклатура", Источник);
//НайденныеСтроки = Параметры.ТабАлкогольнаяПродукцияНоменклатуры.НайтиСтроки(ПараметрыПоиска);

//Если НайденныеСтроки.Количество() &gt; 0 Тогда
//	ВходящиеДанные.Вставить("АлкогольнаяПродукция",    НайденныеСтроки[0].АлкогольнаяПродукция);
//	ВходящиеДанные.Вставить("ВидАлкогольнойПродукции", НайденныеСтроки[0].ВидАлкогольнойПродукции);
//	ВходящиеДанные.Вставить("ВестиУчетПоГТД",          НайденныеСтроки[0].ВестиУчетПоГТД);
//Иначе
//	// ВидАлкогольнойПродукции и АлкогольнаяПродукция
//	Запрос = Новый Запрос;
//	Запрос.Текст = "
//		|ВЫБРАТЬ
//		|	СведенияОбАлкогольнойПродукции.ВидПродукции,
//		|	СведенияОбАлкогольнойПродукции.ВидЛицензии
//		|ИЗ
//		|	РегистрСведений.СведенияОбАлкогольнойПродукции КАК СведенияОбАлкогольнойПродукции
//		|ГДЕ
//		|	СведенияОбАлкогольнойПродукции.Номенклатура = &amp;Номенклатура";
//		
//	Запрос.УстановитьПараметр("Номенклатура", Источник.Ссылка);

//	Выборка = Запрос.Выполнить().Выбрать();
//	Если Выборка.Следующий() Тогда
//		ВходящиеДанные.Вставить("АлкогольнаяПродукция", Истина);
//		ВходящиеДанные.Вставить("ВидАлкогольнойПродукции", Выборка.ВидПродукции);
//	Иначе
//		ВходящиеДанные.Вставить("АлкогольнаяПродукция", Ложь);
//		ВходящиеДанные.Вставить("ВидАлкогольнойПродукции", Справочники.ВидыАлкогольнойПродукции.ПустаяСсылка());
//	КонецЕсли;
	
	// ВестиУчетПоГТД
	//++БК3
	Если Ложь Тогда//Не Источник.ЭтоГруппа Тогда
	//--БК3
		Запрос = Новый Запрос("
		|ВЫБРАТЬ ПЕРВЫЕ 1
		|	1
		|ИЗ
		|	Справочник.СерииНоменклатуры КАК СерииНоменклатуры
		|ГДЕ
		|	СерииНоменклатуры.НомерГТД &lt;&gt; ЗНАЧЕНИЕ(Справочник.НомераГТД.ПустаяСсылка)
		|	И СерииНоменклатуры.Владелец = &amp;Номенклатура
		|");

		Запрос.УстановитьПараметр("Номенклатура", Источник.Ссылка);

		ВходящиеДанные.Вставить("ВестиУчетПоГТД", Запрос.Выполнить().Пустой());
	Иначе
		ВходящиеДанные.Вставить("ВестиУчетПоГТД", Ложь);
	КонецЕсли;
//КонецЕсли;

Если ЗначениеЗаполнено(Источник.Ссылка) И Не Источник.ЭтоГруппа И ЗначениеЗаполнено(ВходящиеДанные.ГруппаФинансовогоУчета) Тогда
	
	НоваяСтрокаГФУ = Параметры.ПараметрыАлгоритмов.НоменклатураПоГФУ.Добавить();
	НоваяСтрокаГФУ.Номенклатура = Источник.Ссылка;
	НоваяСтрокаГФУ.ГФУ          = ВходящиеДанные.ГруппаФинансовогоУчета;
	Если ВходящиеДанные.Свойство("СчетУчетаНаСкладе") Тогда
		НоваяСтрокаГФУ.СчетУчетаНаСкладе = ВходящиеДанные.СчетУчетаНаСкладе;
	Иначе
		НоваяСтрокаГФУ.СчетУчетаНаСкладе = Неопределено;
	КонецЕсли;

КонецЕсли;</Текст>
				<Параметры>Источник, ВходящиеДанные</Параметры>
			</Алгоритм>
			<Алгоритм Имя="ВыгрузкаСвязейСтатейПоказателей" ИспользуетсяПриЗагрузке="false">
				<Текст>Запрос = Новый Запрос("
|ВЫБРАТЬ
|	СтатьиОборотовПоБюджетамШаблоныПроводок.Ссылка КАК СтатьяБюджетов,
|	СтатьиОборотовПоБюджетамШаблоныПроводок.СчетДт КАК СвязанныйПоказательБюджетов,
|	ЛОЖЬ КАК Расход,
|	1 КАК Коэффициент
|ИЗ
|	Справочник.СтатьиОборотовПоБюджетам.ШаблоныПроводок КАК СтатьиОборотовПоБюджетамШаблоныПроводок
|
|ОБЪЕДИНИТЬ ВСЕ
|
|ВЫБРАТЬ
|	СтатьиОборотовПоБюджетамШаблоныПроводок.Ссылка,
|	СтатьиОборотовПоБюджетамШаблоныПроводок.СчетКт,
|	ИСТИНА,
|	1
|ИЗ
|	Справочник.СтатьиОборотовПоБюджетам.ШаблоныПроводок КАК СтатьиОборотовПоБюджетамШаблоныПроводок");

Таблица = Запрос.Выполнить().Выгрузить();
Для Каждого Строка Из Таблица Цикл
	
	СвязьСтатейПоказателей = Новый Структура;
	Для Каждого Колонка Из Таблица.Колонки Цикл
		СвязьСтатейПоказателей.Вставить(Колонка.Имя, Строка[Колонка.Имя]);
	КонецЦикла;	
	ВыгрузитьПоПравилу(СвязьСтатейПоказателей,,,,"СвязиПоказателейБюджетов");	
	
КонецЦикла;</Текст>
			</Алгоритм>
			<Алгоритм Имя="ВыгрузкаЭтаповПроизводства" ИспользуетсяПриЗагрузке="false">
				<Текст>Запрос = Новый Запрос;
Запрос.Текст = 
	"ВЫБРАТЬ
	|	ТехнологическиеКартыСпецификацийПланированияСрезПоследних.ТехнологическаяКарта
	|ИЗ
	|	РегистрСведений.ТехнологическиеКартыСпецификацийПланирования.СрезПоследних(, Спецификация = &amp;Спецификация) КАК ТехнологическиеКартыСпецификацийПланированияСрезПоследних";
	
Спецификация = Неопределено;
Если ТипЗнч(Источник) = Тип("СправочникСсылка.СпецификацииНоменклатуры") Тогда
	Спецификация = Источник;
Иначе
	Спецификация = Источник.Спецификация;
КонецЕсли;

Запрос.УстановитьПараметр("Спецификация", Спецификация);
Выборка = Запрос.Выполнить().Выбрать();

Если Выборка.Следующий() Тогда
	
	Подразделение = ОбщегоНазначения.ПолучитьЗначениеРеквизита(Выборка.ТехнологическаяКарта, "Подразделение");
	ИсходящиеДанные = Новый Структура("МаршрутнаяКарта, Подразделение", Выборка.ТехнологическаяКарта, Подразделение);
	ВыгрузитьПоПравилу(Спецификация, , ИсходящиеДанные, ИсходящиеДанные, "ЭтапыПроизводства");	
КонецЕсли;</Текст>
				<Параметры>Источник</Параметры>
			</Алгоритм>
			<Алгоритм Имя="ДобавитьСтрокиВыборкаДанныхКассы" ИспользуетсяПриЗагрузке="false">
				<Текст>ТаблицаОстатков.Сортировать("Организация Возр");

ОрганизацияДокумента = Неопределено;
НоваяСтрока          = Неопределено;

Для Каждого Строка Из ТаблицаОстатков Цикл
	
	Если Строка.Организация &lt;&gt; ОрганизацияДокумента Тогда
		ОрганизацияДокумента = Строка.Организация;
		
		Если НоваяСтрока &lt;&gt; Неопределено И НоваяСтрока[ИмяТабличнойЧасти].Количество() = 0 Тогда
			ВыборкаДанных.Удалить(НоваяСтрока);			
		КонецЕсли;		
		
		НоваяСтрока = ВыборкаДанных.Добавить();
		НоваяСтрока.Дата        = КонецДня(Дата("20221231235959")); 
		НоваяСтрока.Организация = ОрганизацияДокумента;
		
		НоваяСтрока[ИмяТабличнойЧасти] = Новый ТаблицаЗначений;
		НоваяСтрока[ИмяТабличнойЧасти].Колонки.Добавить("Касса");
		НоваяСтрока[ИмяТабличнойЧасти].Колонки.Добавить("БанковскийСчет");
		НоваяСтрока[ИмяТабличнойЧасти].Колонки.Добавить("Сумма");
		НоваяСтрока[ИмяТабличнойЧасти].Колонки.Добавить("СуммаРегл");
		НоваяСтрока[ИмяТабличнойЧасти].Колонки.Добавить("СуммаУпр");
		НоваяСтрока[ИмяТабличнойЧасти].Колонки.Добавить("ГруппаФинансовогоУчета");
	КонецЕсли;
	
	Если Строка.Сумма &lt; 0.00 И Строка.СуммаРегл &lt; 0.00 Тогда
		СтрокаСообщения = НСтр("ru = 'Обнаружены отрицательные остатки по ""%1"" при выгрузке по правилу: ""%2"".'");
		Если ИмяТабличнойЧасти = "Кассы" Тогда
			СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%1", Строка.Касса);
			СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%2", "Денежные средства в кассах");
		Иначе
			СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%1", Строка.БанковскийСчет);
			СтрокаСообщения = СтрЗаменить(СтрокаСообщения, "%2", "Денежные средства на банковских счетах");
		КонецЕсли;
		
		ЗаписатьВПротоколВыполнения(СтрокаСообщения,,Ложь);
		Сообщить("АЛГОРИТМ ДобавитьСтрокиВыборкаДанныхКассы, условие 1: " + СтрокаСообщения);
		Продолжить;
	КонецЕсли;
	
	НоваяСтрокаТЧ = НоваяСтрока[ИмяТабличнойЧасти].Добавить();
	ЗаполнитьЗначенияСвойств(НоваяСтрокаТЧ, Строка);
		
	НоваяСтрокаТЧ.ГруппаФинансовогоУчета = Строка.Счет;
	
	Если ИмяТабличнойЧасти = "Кассы" И Не ЗначениеЗаполнено(Строка.Касса) Тогда // это неизвестная касса должна быть
		
		// Оказалось, что несколько касс в одной валюте у этой организации. Поэтому создаем еще одну Неизвестную.
		 НеизвестнаяКасса = Новый Структура;
		 НеизвестнаяКасса.Вставить("ВалютаДенежныхСредств", Строка.Валюта);
		 НеизвестнаяКасса.Вставить("Владелец", Строка.Организация);
		 НеизвестнаяКасса.Вставить("Наименование", "Неизвестная касса");
		 НеизвестнаяКасса.Вставить("ПометкаУдаления", Ложь);
		 НеизвестнаяКасса.Вставить("ЭтоГруппа", Ложь);
		 НеизвестнаяКасса.Вставить("Ссылка", Неопределено);
		 
		 НоваяСтрокаТЧ.Касса = НеизвестнаяКасса;
		 
		 //++КАКЗ
	 //ИначеЕсли ИмяТабличнойЧасти = "Кассы" И Не ЗначениеЗаполнено(Строка.Касса) Тогда
	 ИначеЕсли ИмяТабличнойЧасти = "БанковскиеСчета" И Не ЗначениеЗаполнено(Строка.БанковскийСчет) Тогда
		 //--КАКЗ
		 
		 НеизвестнаяКасса = Новый Структура;
		 НеизвестнаяКасса.Вставить("ВалютаДенежныхСредств", Строка.Валюта);
		 НеизвестнаяКасса.Вставить("Владелец", Строка.Организация);
		 НеизвестнаяКасса.Вставить("Наименование", "Неизвестный банковский счет");
		 НеизвестнаяКасса.Вставить("ПометкаУдаления", Ложь);
		 НеизвестнаяКасса.Вставить("ЭтоГруппа", Ложь);
		 НеизвестнаяКасса.Вставить("Ссылка", Неопределено);
		 
		 НоваяСтрокаТЧ.БанковскийСчет = НеизвестнаяКасса;
		 
	КонецЕсли;	
	
КонецЦикла;</Текст>
				<Параметры>ИмяТабличнойЧасти, ТаблицаОстатков, ВыборкаДанных</Параметры>
			</Алгоритм>
			<Алгоритм Имя="ДобавитьСтрокиВыборкиДолгиКлиентовПоставщиков" ИспользуетсяПриЗагрузке="false">
				<Текст>ТаблицаОстатков.Сортировать("Организация Возр");
ОрганизацияДокумента = Неопределено;
НоваяСтрока          = Неопределено;
ИмяПравила           = Параметры.ПараметрыАлгоритмов.ИмяТекущегоМассиваОстатков;
		
Если ИмяПравила = "КредитыЗаймыКлиентов" Или ИмяПравила = "КредитыЗаймыПоставщиков" Тогда
	ИмяТЧДокумента = "РасчетыПоДоговорамКредитовИДепозитов";
Иначе
	ИмяТЧДокумента = "РасчетыСПартнерами";
КонецЕсли;

Для Каждого Строка Из ТаблицаОстатков Цикл
	Если Окр(Строка.Сумма, 2) &lt; 0 Тогда
		//Сообщить("Условие суммы меньше 0: " + Строка(Строка.Сумма));
		//Продолжить;
	КонецЕсли;
	
	Если НоваяСтрока = Неопределено Или ОрганизацияДокумента &lt;&gt; Строка.Организация Тогда
		ОрганизацияДокумента = Строка.Организация;
			
		НоваяСтрока = ВыборкаДанных.Добавить();
		НоваяСтрока.Дата        = КонецДня(Дата("20221231235959")); 
		НоваяСтрока.Организация = Строка.Организация;
		
		Если ИмяПравила = "АвансыКлиентов" Тогда
			НоваяСтрока.ТипОперации = "ОстаткиПоАвансамКлиентов";	
		ИначеЕсли ИмяПравила = "ДолгиКлиентов" Тогда
			НоваяСтрока.ТипОперации = "ОстаткиЗадолженностиКлиентов";
		ИначеЕсли ИмяПравила = "АвансыПоставщикам" Тогда
			НоваяСтрока.ТипОперации = "ОстаткиПоАвансамПоставщикам";	
		ИначеЕсли ИмяПравила = "ДолгиПоставщикам" Тогда
			НоваяСтрока.ТипОперации = "ОстаткиЗадолженностиПередПоставщиками";		
		ИначеЕсли ИмяПравила = "КредитыЗаймыКлиентов" Или ИмяПравила = "КредитыЗаймыПоставщиков" Тогда
			НоваяСтрока.ТипОперации = "ОстаткиПоДоговорамКредитовИДепозитов";		
		КонецЕсли;		
				
		НоваяСтрока[ИмяТЧДокумента] = Новый ТаблицаЗначений;
		НоваяСтрока[ИмяТЧДокумента].Колонки.Добавить("Счет");
		НоваяСтрока[ИмяТЧДокумента].Колонки.Добавить("ТипСрочности");
		НоваяСтрока[ИмяТЧДокумента].Колонки.Добавить("Договор");
		НоваяСтрока[ИмяТЧДокумента].Колонки.Добавить("Партнер");
		НоваяСтрока[ИмяТЧДокумента].Колонки.Добавить("Контрагент");
		НоваяСтрока[ИмяТЧДокумента].Колонки.Добавить("ДатаРасчетногоДокумента");
		НоваяСтрока[ИмяТЧДокумента].Колонки.Добавить("НомерРасчетногоДокумента");
		НоваяСтрока[ИмяТЧДокумента].Колонки.Добавить("ОбъектРасчетов");
		НоваяСтрока[ИмяТЧДокумента].Колонки.Добавить("Сумма");
		НоваяСтрока[ИмяТЧДокумента].Колонки.Добавить("СуммаРегл");
		НоваяСтрока[ИмяТЧДокумента].Колонки.Добавить("СуммаУпр");
		НоваяСтрока[ИмяТЧДокумента].Колонки.Добавить("ВалютаВзаиморасчетов");
		НоваяСтрока[ИмяТЧДокумента].Колонки.Добавить("ДатаПлатежа");
		НоваяСтрока[ИмяТЧДокумента].Колонки.Добавить("Сделка");
		НоваяСтрока[ИмяТЧДокумента].Колонки.Добавить("ДокументРасчетовСКонтрагентом");
		НоваяСтрока[ИмяТЧДокумента].Колонки.Добавить("ДокументРасчетов");
	КонецЕсли;
	
	НоваяСтрокаРасчетыСПартнерами = НоваяСтрока[ИмяТЧДокумента].Добавить();
	ЗаполнитьЗначенияСвойств(НоваяСтрокаРасчетыСПартнерами, Строка);
	
	Если ЗначениеЗаполнено(НоваяСтрокаРасчетыСПартнерами.Сделка) Тогда
		НоваяСтрокаРасчетыСПартнерами.ОбъектРасчетов = НоваяСтрокаРасчетыСПартнерами.Сделка;
	КонецЕсли;
	
	СтруктураПД = Неопределено;
	Договор     = Неопределено;
	Если ЗначениеЗаполнено(Строка.ОбъектРасчетов)
		и ТипЗнч(Строка.ОбъектРасчетов) = Тип("СправочникСсылка.ДоговорыКонтрагентов") Тогда
		Договор = Строка.ОбъектРасчетов;
	КонецЕсли;
	
	СтруктураДоговора = Неопределено;
	Если ЗначениеЗаполнено(Договор) 
		И Договор.Организация &lt;&gt; ОрганизацияДокумента Тогда
		// Это ситуация, когда создаем остатки по упр. учету, которые не найдены в регл. учете
		// ни на одном из субсчетов. В этом случае организация - Управленческая организация.
		// Для нее необходимо создать договоры с партнерами-клоны тех, что были для старых организаций.
		СтруктураДоговора = Новый Структура("Наименование, Контрагент, Организация, ТипДоговора, ВалютаВзаиморасчетов, " +
		"Дата, ДатаОкончанияДействия, Номер, Код, Комментарий, ПорядокРасчетов, ПорядокОплаты, ДопустимаяСуммаЗадолженности, ЭтоГруппа, ПометкаУдаления, " +
		"ВидАгентскогоДоговора, УчетАгентскогоНДС, ЗапрещаетсяПросроченнаяЗадолженность, СтатьяДвиженияДенежныхСредств, ХозяйственнаяОперация");
		
//++БК3		
		//ErpCompany 05.07.2022
		ЕстьКонтролироватьСуммуЗадолженности = ОбщегоНазначенияКлиентСервер.ЕстьРеквизитИлиСвойствоОбъекта(Договор, "КонтролироватьСуммуЗадолженности");
		//Сообщить("ЕстьКонтролироватьСуммуЗадолженности: " + ЕстьКонтролироватьСуммуЗадолженности);
		//***ErpCompany 05.07.2022 ***
		ДанныеДоговора = ОбщегоНазначения.ПолучитьЗначенияРеквизитов(Договор, "Владелец, ВидДоговора, ДатаОкончанияДействияДоговора, ВедениеВзаиморасчетов" +
			?(ЕстьКонтролироватьСуммуЗадолженности, ", КонтролироватьСуммуЗадолженности", ""));
//--БК3
	   
		ЗаполнитьЗначенияСвойств(СтруктураДоговора, Договор);
		СтруктураДоговора.Организация = Справочники.Организации.ПустаяСсылка();
		СтруктураДоговора.Контрагент  = ДанныеДоговора.Владелец;
		СтруктураДоговора.ТипДоговора = ДанныеДоговора.ВидДоговора;
		СтруктураДоговора.ХозяйственнаяОперация = ДанныеДоговора.ВидДоговора;
//++УТП
		СтруктураДоговора.ДатаОкончанияДействия = ДанныеДоговора.ДатаОкончанияДействияДоговора;
//--УТП
		СтруктураДоговора.ПорядокРасчетов       = ДанныеДоговора.ВедениеВзаиморасчетов;
//++УТП		
//		СтруктураДоговора.ПорядокОплаты         = ДанныеДоговора.УдалитьРасчетыВУсловныхЕдиницах;
//--УТП
		//ErpCompany 05.07.2022
		СтруктураДоговора.ЗапрещаетсяПросроченнаяЗадолженность = ?(ЕстьКонтролироватьСуммуЗадолженности, ДанныеДоговора.КонтролироватьСуммуЗадолженности, Ложь);
		//***ErpCompany 05.07.2022 ***
		//++БК3
		СтруктураДоговора.СтатьяДвиженияДенежныхСредств = Справочники.СтатьиДвиженияДенежныхСредств.ПустаяСсылка();
		//--БК3
		
	   
		НоваяСтрокаРасчетыСПартнерами.ОбъектРасчетов = СтруктураДоговора;
		
	КонецЕсли;
	
	Если НЕ НоваяСтрока.ТипОперации = "ОстаткиПоДоговорамКредитовИДепозитов" Тогда
		Если ЗначениеЗаполнено(НоваяСтрокаРасчетыСПартнерами.ДокументРасчетовСКонтрагентом)
			и ОбщегоНазначения.ЭтоДокумент(НоваяСтрокаРасчетыСПартнерами.ДокументРасчетовСКонтрагентом.Метаданные()) Тогда
			
			ДокументСсылка = НоваяСтрокаРасчетыСПартнерами.ДокументРасчетовСКонтрагентом;
			Выборка = Новый Структура("Организация, Контрагент, Договор");
			ЗаполнитьЗначенияСвойств(Выборка, Строка);
			Если ЗначениеЗаполнено(СтруктураДоговора) Тогда
				Выборка.Договор = СтруктураДоговора;	
			ИначеЕсли ЗначениеЗаполнено(Договор) Тогда
				Выборка.Договор = Договор;
			КонецЕсли;
			
			Выполнить(Алгоритмы.ПодготовитьСтруктуруДанныхПервичногоДокумента);
		Иначе
			СтруктураПД = Новый Структура("Организация, Контрагент, Договор, Валюта, СуммаДокумента, СуммаРегл, ДатаВходящегоДокумента, НомерВходящегоДокумента, Подразделение, Комментарий, ТипПервичногоДокумента");
			ЗаполнитьЗначенияСвойств(СтруктураПД, Строка);
			
			Если ЗначениеЗаполнено(СтруктураДоговора) Тогда
				СтруктураПД.Договор = СтруктураДоговора;
			ИначеЕсли ЗначениеЗаполнено(Договор) Тогда
				СтруктураПД.Договор = Договор;
			КонецЕсли;
			СтруктураПД.СуммаДокумента = НоваяСтрокаРасчетыСПартнерами.Сумма;
			СтруктураПД.Валюта = НоваяСтрокаРасчетыСПартнерами.ВалютаВзаиморасчетов;
			СтруктураПД.ДатаВходящегоДокумента = НоваяСтрокаРасчетыСПартнерами.ДатаРасчетногоДокумента;
			СтруктураПД.НомерВходящегоДокумента = НоваяСтрокаРасчетыСПартнерами.НомерРасчетногоДокумента;
			Если ИмяПравила = "ДолгиКлиентов" Тогда
				СтруктураПД.ТипПервичногоДокумента = "РеализацияКлиенту";
			ИначеЕсли ИмяПравила = "АвансыКлиентов" Тогда
				СтруктураПД.ТипПервичногоДокумента = "ОплатаОтКлиента";
			ИначеЕсли ИмяПравила = "АвансыПоставщикам" Тогда
				СтруктураПД.ТипПервичногоДокумента = "ОплатаПоставщику";
			Иначе
				СтруктураПД.ТипПервичногоДокумента = "ПриобретениеУПоставщика";
			КонецЕсли;

		КонецЕсли;
	КонецЕсли;	
	   
	Если СтруктураПД &lt;&gt; Неопределено Тогда
		НоваяСтрокаРасчетыСПартнерами.ДокументРасчетов = СтруктураПД;
	КонецЕсли;
КонецЦикла;</Текст>
				<Параметры>ТаблицаОстатков, ВыборкаДанных</Параметры>
			</Алгоритм>
			<Алгоритм Имя="ДобавитьСтрокиВыборкиПодотчетныхЛиц" ИспользуетсяПриЗагрузке="false">
				<Текст>ТаблицаОстатков.Сортировать("Организация Возр, Сумма Возр");
ОрганизацияДокумента = Неопределено;
НоваяСтрока          = Неопределено;
ТипОперации          = Неопределено;

Для Каждого Строка Из ТаблицаОстатков Цикл
	
	НоваяСтрокаТипОперации = Неопределено;

	Если Строка.Сумма &lt; 0 Тогда // это перерасход
		НоваяСтрокаТипОперации = "ПерерасходыПодотчетныхСредств";
	Иначе
		НоваяСтрокаТипОперации = "ОстаткиЗадолженностиПодотчетников"; 
	КонецЕсли;

	Если НоваяСтрока = Неопределено Или ОрганизацияДокумента &lt;&gt; Строка.Организация Или НоваяСтрокаТипОперации &lt;&gt; ТипОперации Тогда
		ОрганизацияДокумента = Строка.Организация;
		ТипОперации = НоваяСтрокаТипОперации;
		
		Если НоваяСтрока &lt;&gt; Неопределено И НоваяСтрока.РасчетыСПодотчетниками.Количество() = 0 Тогда
			ВыборкаДанных.Удалить(НоваяСтрока);
		КонецЕсли;
	
		НоваяСтрока = ВыборкаДанных.Добавить();
		НоваяСтрока.Дата        = КонецДня(Дата("20221231235959")); 
		НоваяСтрока.Организация = Строка.Организация;
		НоваяСтрока.ТипОперации = ТипОперации;
				
		НоваяСтрока.РасчетыСПодотчетниками = Новый ТаблицаЗначений;
		НоваяСтрока.РасчетыСПодотчетниками.Колонки.Добавить("ПодотчетноеЛицо");
		НоваяСтрока.РасчетыСПодотчетниками.Колонки.Добавить("Сумма");
		НоваяСтрока.РасчетыСПодотчетниками.Колонки.Добавить("СуммаРегл");
		НоваяСтрока.РасчетыСПодотчетниками.Колонки.Добавить("СуммаУпр");
		НоваяСтрока.РасчетыСПодотчетниками.Колонки.Добавить("Валюта");
		
	КонецЕсли;	
	
	НоваяСтрокаРасчетыСПодотчетниками = НоваяСтрока.РасчетыСПодотчетниками.Добавить();
	Если Строка.Сумма &lt; 0 Тогда
		Строка.Сумма = -Строка.Сумма;
	КонецЕсли;
	Если Строка.СуммаРегл &lt; 0 Тогда
		Строка.СуммаРегл = -Строка.СуммаРегл;
	КонецЕсли;
	Если Строка.СуммаУпр &lt; 0 Тогда
		Строка.СуммаУпр = -Строка.СуммаУпр;
	КонецЕсли;

	
	ЗаполнитьЗначенияСвойств(НоваяСтрокаРасчетыСПодотчетниками, Строка);
	
